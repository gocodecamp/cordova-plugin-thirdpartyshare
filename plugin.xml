                <key>wechatSessionAppKey</key>
                <string>$UM_WECHAT_KEY</string>
                <key>appKey</key>
                <string>$UM_KEY_IOS</string>
                <key>sinaRedirectURL</key>
                <string>$UM_SINA_REDIRECT_URL</string>
            </dict>
        </config-file>
